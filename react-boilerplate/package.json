{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --hot --inline --history-api-fallback --port 3000",
    "prestart": "NODE_ENV=dev && webpack",
    "build": "export NODE_ENV=production && webpack",
    "save": "git add . && git commit -m \"Quick save\" && git push",
    "lint": "eslint components/* --ext .js --ext .jsx --cache --fix -f html -o test.html",
    "test": "jest --verbose --watch",
    "debugtest": "node-debug --nodejs --harmony ./node_modules/jest-cli/bin/jest.js --runInBand "
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-redux/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ],
    "moduleNameMapper": {
      "^[./a-zA-Z0-9$_-]+\\.scss$": "<rootDir>/SCSSStub.js"
    }
  },
  "author": "Vaughn Angoy <vaughnydream@gmail.com> (http://www.vaughnangoy.com)",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.11.0",
    "react": "^15.1.0",
    "react-dom": "^15.0.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2",
    "redux-form": "^5.2.4",
    "redux-thunk": "^2.1.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.8.0",
    "babel-jest": "^12.1.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.10.0",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "img-loader": "^1.2.2",
    "jest-cli": "^12.1.1",
    "json-loader": "^0.5.4",
    "node-sass": "^3.7.0",
    "object-merge": "^2.5.1",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^3.2.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
